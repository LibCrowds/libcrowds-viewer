{
  "name": "libcrowds-viewer-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "clean": "rimraf ./dist",
    "prepare": "npm run clean && npm run build && sed 's|dist/|libcrowds-viewer/|g' index.html > dist/index.html",
    "deploy": "npm run prepare && gh-pages -d dist/"
  },
  "dependencies": {
    "balloon-css": "^0.4.0",
    "hint.css": "^2.5.0",
    "vue": "^2.0.0",
    "vue-awesome": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
